@article{evans_developmental_2016,
	title = {The {Developmental} {Effect} of {State} {Alcohol} {Prohibitions} at the {Turn} of the {Twentieth} {Century}},
	volume = {54},
	copyright = {© 2015 Western Economic Association International},
	issn = {1465-7295},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ecin.12303},
	doi = {10.1111/ecin.12303},
	abstract = {We examine the quasi-randomization of alcohol consumption created by state-level alcohol prohibition laws passed in the United States in the early part of the twentieth century. Using a large dataset of World War II enlistees, we exploit the differential timing of these laws to examine their effects on adult educational attainment, obesity, and height. We find statistically significant effects for education and obesity that do not appear to be the result of pre-existing trends. Our findings add to the growing body of economic studies that examine the long-run impacts of in utero and childhood environmental conditions. (JEL I18, D10, N41, N42)},
	language = {en},
	number = {2},
