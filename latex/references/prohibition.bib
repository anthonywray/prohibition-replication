	number = {19},
	journal = {National Vital Statistics Reports},
	author = {Miniño, Arialdi M. and Heron, Melonie P. and Murphy, Sherry L. and Kochanek, Kenneth D. and {Centers for Disease Control and Prevention National Center for Health Statistics National Vital Statistics System}},
